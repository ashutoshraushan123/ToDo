<!DOCTYPE html>
<html>
<head>
  <title>Todo</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
	<div id="wrap"> 
	 <div class="navbar navbar-fluid-top ">
	  <div class="navbar-inner nav-bg">
	  	<div class="container">
		  	<a class="btn btn-navbar" data-target=".nav-collapse" data-toggle="collapse">
		            <span class="icon-bar"></span>
		            <span class="icon-bar"></span>
		            <span class="icon-bar"></span>
		    </a>
		    <a class="pull-left" href="#"></a>
			<div class="container-fluid nav-collapse collapse">
				<div class="nav nav-tabs pull-left" id="myTab">
					<h1>ToDo<h1>      
		            <p>Welcome to the Your Working Diary</p>      
				</div>
			</div><!--/.nav-collapse -->
			<div class="clearfix"></div>
	    </div>
	  </div>
	 </div>
	 <div class="container-fluid">
		<div class="row-fluid">  
		 <div class="span12"> 
		  <div class="container">  	  
		   <%= yield %>
		   </div> 
		 </div> 
		</div> 
	 </div>
	 <div id="push"></div>
	</div>
	<div class="footer">	
	  <div class="container">
		<p class="credit" >Example courtesy:Ashutosh Raushan </p>
	  </div>
	</div>
</body>
</html>
